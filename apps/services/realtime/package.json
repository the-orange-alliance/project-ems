{
  "name": "realtime",
  "version": "1.0.0",
  "description": "Realtime service for EMS.",
  "main": "build/Server.js",
  "type": "module",
  "scripts": {
    "start": "tsc -p . && node build/Server.js",
    "dev": "set SERVICE_PORT=8081 && npm run start",
    "build": "tsc -p .",
    "dist": "npm run dist:index && npm run dist:worker",
    "dist:index": "ncc build build/Server.js -o dist",
    "dist:worker": "ncc build build/util/WLEDWorker/worker.js -o dist/worker"
  },
  "author": "Kyle Flynn",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.0",
    "@vercel/ncc": "^0.38.3",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@toa-lib/models": "1.0.0",
    "@toa-lib/server": "1.0.0",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "node-inline-worker": "^1.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0",
    "ws": "^8.18.1"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}
